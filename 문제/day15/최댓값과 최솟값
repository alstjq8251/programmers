    public String solution(String s) {
        String[] number = s.split(" ");
        int min =1000000;
        int max = -100000;
        for(int i=0; i<number.length; i++){
            min = Math.min(min,Integer.parseInt(number[i]));
            max = Math.max(max,Integer.parseInt(number[i]));
        }
        String answer = min + " " + max;
        return answer;
    }
